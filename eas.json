{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_NAME": "Field Operations MS (Development)",
        "PACKAGE_NAME": "xyz.djcayz.foms.dev",
        "API_URL": "http://localhost:3000"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_NAME": "Field Operations MS (Internal)",
        "PACKAGE_NAME": "xyz.djcayz.foms.internal",
        "API_URL": "https://foms.djcayz.xyz"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_NAME": "Field Operations MS",
        "PACKAGE_NAME": "xyz.djcayz.fomsmobile",
        "API_URL": "https://foms.djcayz.xyz"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
