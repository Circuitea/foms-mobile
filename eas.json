{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://localhost:3000/api/"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://foms.djcayz.xyz/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://foms.djcayz.xyz/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
